pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building the project'
                
            }
        }
    }
  post {
    success {
    emailext body: ''' 
      Job Name: $JOB_NAME
      Build Number: $BUILD_NUMBER
      Job URL: $BUILD_URL
      Build Status: SUCCESS
      '''
    subject: '$JOB_NAME - Build # $BUILD_NUMBER - Success', to: 'korangisairamesh0@gmail.com'
}
}
}
     

